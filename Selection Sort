int select(int arr[], int i,int n)
    {
            int min_index=i;
            for(int j=i+1;j<n;j++){
                if(arr[min_index]>arr[j]){
                    min_index=j;
                }
            }
        return min_index;
    }
     
    void selectionSort(int arr[], int n)
    {
       for(int i=0;i<n;i++){
           int min_ind=select(arr,i,n);
           if(min_ind!=i)
            swap(arr[min_ind],arr[i]);
       }
    }
